   "source": [
    "Write a program to cyclically rotate an array by one."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "junior-latvia",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "9 2 5 8 3 "
     ]
    }
   ],
   "source": [
    "arr = [2, 5, 8, 3, 9]\n",
    "\n",
    "n = len(arr)\n",
    "\n",
    "for i in range(n):\n",
    "    i = i - 1\n",
    "    print(arr[i], end = \" \")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "eleven-providence",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[9, 2, 5, 8, 3]\n"
     ]
    }
   ],
   "source": [
    "arr = [2, 5, 8, 3, 9]\n",
    "\n",
    "n = len(arr)\n",
    "a = []\n",
    "\n",
    "for i in range(n):\n",
    "    i = i - 1\n",
    "    a.append(arr[i])\n",
    "    \n",
    "print(a)"
   ]
